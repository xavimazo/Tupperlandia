@model IList<Data.Description>
@{
    Layout = "~/Views/Shared/_LayoutDashboard.cshtml";
}
<div class="container wrapper">
    <table class="highlight centered wrapper"
    <thead>
    <br />
    <div class="center">
    <a href="/Description/Create" class="waves-effect waves-light btn"><i class="material-icons left">description</i>Crear descripcion</a>
    </div>
    <br />
    <tr>
    <th>ID</th>
    <th>Titulo</th>
    <th>Descripcion</th>
    <th>Editar</th>
    <th>Eliminar</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var Description in Model)
    {
        <tr>
        <td>@Description.DescriptionId</td>
        <td>@Description.DescriptionTitle</td>
        <td>@Description.DescriptionText</td>
        <td><a href="/Description/Edit/@Description.DescriptionId"><i class="small material-icons">mode_edit</i></a></td>
        <td><a href="/Description/Delete/@Description.DescriptionId"><i class="small material-icons">delete</i></a></td>
        </tr>
    }
    </tbody>
    </table>
</div>
