@model IList<Data.ShoppingCart>
@{
    Layout = "~/Views/Shared/_LayoutDashboard.cshtml";
}
<div class="wrapper">
    <table class="highlight centered">
        <br />
        <div class="center">
            <a href="/ShoppingCart/Create" class="waves-effect waves-light btn"><i class="material-icons left">shopping_cart</i>Crear carrito de ventas</a>
        </div>
        <br />
        <thead>
            <tr>
                <th>Id</th>
                <th>Monto</th>
                <th>Fecha</th>
                @*<th>Stock</th>*@
                <th>Despacho</th>
                <th>Cliente</th>
                <th>Editar</th>
                <th>Eliminar</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var shoppingCart in Model)
            {
                <tr>
                    <td>@shoppingCart.CartId</td>
                    <td>@shoppingCart.Amount</td>
                    <td>@shoppingCart.CartDate</td>
                    @*<td>@shoppingCart.Stock</td>*@
                    <td>@shoppingCart.Dispatch.DispatchDescription</td>
                    <td>@shoppingCart.Client.ClientName</td>
                    @*<td><a href="#!user"><img width="50" height="50" class="circle" src="@stock.Image"></a></td>*@
                    <td><a href="/shoppingCart/Edit/@shoppingCart.CartId"><i class="small material-icons">mode_edit</i></td>
                    <td><a href="/shoppingCart/Delete/@shoppingCart.CartId"><i class="small material-icons">delete</i></td>
                    <td><a href="#!user"><img width="50" height="50" class="circle" src="~/Content/ProductsImages/EcoTwist500mlVioleta.jpg"></a></td>
                </tr>
            }
        </tbody>
    </table>
</div>