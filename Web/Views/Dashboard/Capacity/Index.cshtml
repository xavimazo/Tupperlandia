@model IList<Data.Capacity>
@{
    Layout = "~/Views/Shared/_LayoutDashboard.cshtml";
}
<style type="text/css">
    .wrapper {
        padding-left: 300px;
    }
</style>
<div class="container wrapper">
    <table class="highlight centered wrapper">
        <br />
        <div class="center">
            <a href="/Capacity/Create" class="waves-effect waves-light btn"><i class="material-icons left">more_vert</i>Crear capcidad</a>
        </div>
        <br />
        <thead>
            <tr>
                <th>ID de la capacidad</th>
                <th>Capacidad</th>
                <th>Editar</th>
                <th>Eliminar</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var Capacity in Model)
            {
                <tr>
                    <td>@Capacity.CapacityId</td>
                    <td>@Capacity.CapacityDescription</td>
                    <td><a href="/Capacity/Edit/@Capacity.CapacityId"><i class="small material-icons">mode_edit</i></a></td>
                    <td><a href="/Capacity/Delete/@Capacity.CapacityId"><i class="small material-icons">delete</i></a></td>
                </tr>
            }
        </tbody>
    </table>
</div>