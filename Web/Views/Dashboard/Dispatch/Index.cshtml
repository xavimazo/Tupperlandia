@model IList<Data.Dispatch>
@{
    Layout = "~/Views/Shared/_LayoutDashboard.cshtml";
}
<div class="container wrapper">
    <table class="highlight centered wrapper">
        <thead>
            <br />
            <div class="center">
                <a href="/Dispatch/Create" class="waves-effect waves-light btn"><i class="material-icons left">gps_fixed</i>Crear espacho</a>
            </div>
            <tr>
                <th>ID</th>
                <th>Tipo de entrega</th>
                <th>Editar</th>
                <th>Eliminar</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var Dispatch in Model)
            {
                <tr>
                    <td>@Dispatch.DispatchId</td>
                    <td>@Dispatch.DispatchDescription</td>
                    <td><a href="/Dispatch/Edit/@Dispatch.DispatchId"><i class="small material-icons">mode_edit</i></a></td>
                    <td><a href="/Dispatch/Delete/@Dispatch.DispatchId"><i class="small material-icons">delete</i></a></td>
                </tr>
            }
        </tbody>
    </table>
</div>

