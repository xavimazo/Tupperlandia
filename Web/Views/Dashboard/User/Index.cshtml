@model IList<Data.User>
@{
    Layout = "~/Views/Shared/_LayoutDashboard.cshtml";
}
<style type="text/css">
    .wrapper {
        padding-left: 300px;
    }
</style>
<div class="container wrapper">
    <table class="highlight centered wrapper">
        <br />
        <div class="center">
            <a href="/User/Create" class="waves-effect waves-light btn"><i class="material-icons left">face</i>Crear usuario</a>
        </div>
        <br />
        <thead>
            <tr>
                <th>ID de usuario</th>
                <th>Nombre del usuario</th>
                <th>Apellido del usuario</th>
                <th>Email</th>
                <th>Contraseña</th>
                <th>Editar</th>
                <th>Eliminar</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var User in Model)
            {
                <tr>
                    <td>@User.UserId</td>
                    <td>@User.Name</td>
                    <td>@User.LastName</td>
                    <td>@User.Email</td>
                    <td>@User.Password</td>
                    <td><a href="/User/Edit/@User.UserId"><i class="small material-icons">mode_edit</i></a></td>
                    <td><a href="/User/Delete/@User.UserId"><i class="small material-icons">delete</i></a></td>
                </tr>
            }
        </tbody>
    </table>
</div>